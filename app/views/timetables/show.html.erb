<%
    start_hour  = @timetable.start_day_time.hour - 1
    start_min   = @timetable.start_day_time.min
    end_hour    = @timetable.end_day_time.hour + 1
    end_min     = @timetable.end_day_time.min

    time_ranges = []
    range_hour = (start_hour .. end_hour).to_a * 2
    half = false
    range_hour.sort.each do |hour|
        time_ = half ? '' : "#{hour}:00"
        half = half ? false : true

        if hour == start_hour
            next if start_min == 30 && time_.length > 3
        end

        if hour == end_hour
            break if end_min == 0 || (end_min == 30 && time_.length > 3)
        end
        time_ranges << time_
    end

    # time_ranges = %w(12am) << ' '
    # (1..11).to_a.each do |i|
    #     time_ranges << "#{i}am"
    #     time_ranges << ' '
    # end
    # time_ranges << '12pm' << ' '
    # (1..11).to_a.each do |i|
    #     time_ranges << "#{i}pm"
    #     time_ranges << ' '
    # end

    beginning_of_weekday  = @timetable.start_day_time.beginning_of_week
    beginning_of_weekday_format = beginning_of_weekday.strftime('%m/%d')
    beginning_of_weekday_day    = beginning_of_weekday.day

    weekdays = [
        {en: 'mon', ko: '월', date: [beginning_of_weekday_format.split('/').first, (beginning_of_weekday_format.split('/').last.to_i + 0).to_s].join('/')},
        {en: 'tue', ko: '화', date: [beginning_of_weekday_format.split('/').first, (beginning_of_weekday_format.split('/').last.to_i + 1).to_s].join('/')},
        {en: 'wed', ko: '수', date: [beginning_of_weekday_format.split('/').first, (beginning_of_weekday_format.split('/').last.to_i + 2).to_s].join('/')},
        {en: 'thu', ko: '목', date: [beginning_of_weekday_format.split('/').first, (beginning_of_weekday_format.split('/').last.to_i + 3).to_s].join('/')},
        {en: 'fri', ko: '금', date: [beginning_of_weekday_format.split('/').first, (beginning_of_weekday_format.split('/').last.to_i + 4).to_s].join('/')},
        {en: 'sat', ko: '토', date: [beginning_of_weekday_format.split('/').first, (beginning_of_weekday_format.split('/').last.to_i + 5).to_s].join('/')},
        {en: 'sun', ko: '일', date: [beginning_of_weekday_format.split('/').first, (beginning_of_weekday_format.split('/').last.to_i + 6).to_s].join('/')}]
    weekdays.unshift({en: 'no-date',ko: '#'})
%>


<section id="timetable">
    <div class="container">
        <div class="row">
            <div class="col-sm-6">
                <h2 style="font-weight: bold;"><%= @timetable.group.name %><br>
                    <small style="font-size: 14px; font-weight: bold; color: #777">
                        <%= publish_method_label %>
                    </small>
                </h2>
            </div>
            <div class="col-sm-6">
                <!-- Single button -->
                <div class="btn-group pull-right" style="margin-top: 30px;margin-bottom: 9px;">
                    <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                        more <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu pull-right" role="menu">
                        <li><a href="#">테이블 상세 설정</a></li>
                        <li><a href="#">Another action</a></li>
                        <li><a href="#">Something else here</a></li>
                        <li class="divider"></li>
                        <li><a class="bgm-red" href="#" style="color: #FFFFFF;">테이블 삭제하기</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                <div class="card">
                    <table id="table" class="table table-bordered">
                        <thead class="header">
                        <tr>
                            <th class="day day_head" style="padding: 0;">
                                <a href="#" class="btn btn-default waves-effect" onclick="return false;">
                                    <span class="zmdi zmdi-chevron-left"></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                    <span class="zmdi zmdi-chevron-right"></span>
                                </a>
                            </th>
                            <% today = Date.today %>
                            <th class="day day_head" colspan="6"><%= today.strftime('%Y년 %m월') %> <%= @timetable.end_day_time.strftime('%W').to_i - @timetable.end_day_time.prev_month.end_of_month.strftime('%W').to_i %>번째 주 : <%= today.beginning_of_week.day %>일 ~ <%= today.end_of_week.day %>일</th>
                            <th class="day day_head" style="padding: 0;">
                                <select name="" id="" style="color: #000; font-size: 10px;">
                                    <option value="0">하루 길이 설정</option>
                                    <% i=0; time_ranges.each do |time| %>
                                        <% i += 1 %>
                                        <% next if time.length < 2 %>
                                        <option value="<%= i %>"><%= time %></option>
                                    <% end %>
                                    <%# raise :test %>
                                </select>
                            </th>
                        </tr>
                        <tr>
                            <% weekdays.each do |weekday| %>
                                <th class="day day_head <%= weekday[:en] %>"><%= weekday[:ko] %></th>
                            <% end %>
                        </tr>
                        </thead>
                        <tbody>
                        <% time_ranges.each do |time| %>
                            <tr>
                                <% weekdays.each do |weekday| %>
                                    <td data-weekday="<%= weekday[:en] %>" class="day <%= weekday[:en] %> <%= 'time_cell' unless weekday[:en] == 'no-date' %>"><%== weekday[:ko] == '#' ? time : '&nbsp;' %></td>
                                <% end %>
                            </tr>
                        <% end %>
                        </tbody>
                    </table>
                </div>
            </div>

            <!-- 편의/기능 위젯 그룹 -->
            <div class="col-md-4">

                <!-- 가장 많은 사람들이 되는 시간 -->
                <div class="card rating-list">
                    <div class="listview">
                        <div class="lv-header bgm-indigo">
                            <div class="m-t-5" style="color: #FFFFFF; font-weight: bold;">
                                되는 시간 ** 1위 **<br><br>2017-05-20 / 13:00 ~ 15:00<br><br>
                            </div>

                            <div class="clearfix"></div>

                            <div class="rl-star">
                                <span style="color: #FFFFFF;"><%= @marked_users.count %>명</span>
                                <% marked_counter_alive = true %>
                                <% loop_count = 0; @users.each do |user| %>
                                    <% marked_counter_alive = false if loop_count >= @marked_users.count %>
                                    <i class="zmdi zmdi-star <%= 'active' if marked_counter_alive %>"></i>
                                <% loop_count += 1; end %>
                                <span style="color: #FFFFFF;">총 <%= @users.count %>명</span>
                            </div>
                            <span style="color: #FFFFFF;"><%= @users.pluck(:name).join(', ') %></span>
                            <br><br>
                        </div>

                        <div class="lv-body">
                            <div class="p-15">
