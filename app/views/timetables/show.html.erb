                <div class="card">
                    <div class="card-header bgm-indigo">
                        <h2>참여중인 사람
                            <small>
                                <% color_classes = %w(success warning info danger default) %>
                                <% @users.each do |user| %>
                                    <span class="label label-<%= color_classes.sample %>"><%= user.name %></span>
                                <% end %>
                            </small>
                        </h2>
                    </div>
                </div>

                <!-- 그룹 초대 방법 -->
                <div class="card">
                    <div class="card-header bgm-indigo">
                        <h2>그룹 <%= invite_method %>
                            <small><%== invite_lead_message %></small>
                        </h2>
                    </div>
                    <div class="card-body card-padding">
                        <a id="code_g" href="#" class="btn btn-default bgm-pink w-100" onclick="return false;">전용 코드 발급 받기</a>
                        <span id="code_m" style="display: none"><%= SecureRandom.hex(16) %> &nbsp;&nbsp;&nbsp;
                            <a href="#" class="btn btn-default btn-sm bgm-red" onclick="return false;">숨겨두기</a></span>
                    </div>
                </div>

            </div>
        </div>

    </div>
</section>

